<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd ">

	<bean id="serverDetailsDAO" class="org.mybatis.spring.mapper.MapperFactoryBean">
		<property name="mapperInterface" value="cronmanagement.dao.ServerDetailsDAO" />
		<property name="sqlSessionFactory" ref="cronManagementSqlSessionFactory" />
	</bean>

	<bean id="cronDetailsDAO" class="org.mybatis.spring.mapper.MapperFactoryBean">
		<property name="mapperInterface" value="cronmanagement.dao.CronDetailsDAO" />
		<property name="sqlSessionFactory" ref="cronManagementSqlSessionFactory" />
	</bean>
	<bean id="cronRunHistoryDAO" class="org.mybatis.spring.mapper.MapperFactoryBean">
		<property name="mapperInterface" value="cronmanagement.dao.CronRunHistoryDAO" />
		<property name="sqlSessionFactory" ref="cronManagementSqlSessionFactory" />
	</bean>


</beans>